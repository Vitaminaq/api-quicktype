{
	"name": "api-quicktype",
	"displayName": "API Quicktype",
	"description": "APIQuicktype: yapi,swagger to .d.ts",
	"version": "1.0.4",
	"publisher": "vitaminaq",
	"repository": "https://github.com/Vitaminaq/yapi-quicktype",
	"engines": {
		"vscode": "^1.34.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:api.createType"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "api.createType",
				"title": "APIQuicktype: to .d.ts"
			}
		],
		"keybindings":[
			{
				"command": "api.createType",
				"key": "Ctrl+Shift+7"
			}
		]
	},
	"scripts": {
		"dev": "jiti ./run",
		"start": "jiti index",
		"build": "rollup --config rollup.config.js"
	},
	"dependencies": {
		"axios": "^0.24.0",
		"cli-progress": "^3.9.1",
		"fs-extra": "^10.0.0",
		"inquirer": "^8.2.0",
		"jiti": "^1.12.9",
		"prettier": "^2.5.1",
		"quicktype-core": "^6.0.70",
		"typescript": "^4.7.2"
	},
	"devDependencies": {
		"@babel/core": "^7.18.9",
		"@babel/preset-env": "^7.18.6",
		"@rollup/plugin-babel": "^5.3.1",
		"@rollup/plugin-typescript": "^8.3.3",
		"@types/node": "^16.11.13",
		"rollup": "^2.77.0",
		"rollup-plugin-clear": "^2.0.7",
		"rollup-plugin-terser": "^7.0.2"
	}
}
