{
	"name": "api-quicktype",
	"displayName": "APIQuicktype",
	"description": "APIQuicktype: yapi,swagger to .d.ts",
	"version": "1.0.4",
	"publisher": "vitaminaq",
	"repository": "https://github.com/Vitaminaq/yapi-quicktype",
	"engines": {
		"vscode": "^1.34.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.APIQuicktype"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.APIQuicktype",
				"title": "APIQuicktype: to .d.ts"
			}
		],
		"keybindings":[
			{
				"command": "extension.APIQuicktype",
				"key": "Ctrl+7"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint . --ext .ts,.tsx",
		"watch": "tsc -watch -p ./"
	},
	"dependencies": {
		"axios": "^0.24.0",
		"cli-progress": "^3.9.1",
		"fs-extra": "^10.0.0",
		"inquirer": "^8.2.0",
		"jiti": "^1.12.9",
		"prettier": "^2.5.1",
		"quicktype-core": "^6.0.70",
		"typescript": "^4.7.2"
	},
	"devDependencies": {
		"@types/node": "^16.11.7",
		"@types/readable-stream": "^2.3.14",
		"@types/urijs": "^1.19.19",
		"@types/vscode": "^1.34.0",
		"@typescript-eslint/eslint-plugin": "^5.30.0",
		"@typescript-eslint/parser": "^5.30.0",
		"eslint": "^8.13.0"
	}
}
